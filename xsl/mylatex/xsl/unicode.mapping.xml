<?xml version="1.0"?>
<!-- $Id: unicode.mapping.xml,v 1.1 2005/12/05 08:19:40 rpopov Exp $ -->
<!DOCTYPE mapping SYSTEM "unicode.mapping.dtd">
<mapping>
  <character>
    <unicode>&lt;</unicode>
    <latex>\guilsinglleft </latex>
    <preferred>\guilsinglleft </preferred>
  </character>
    <character>
    <unicode>></unicode>
    <latex>\guilsinglright </latex>
    <preferred>\guilsinglright </preferred>
  </character>
  <character>
    <unicode>#</unicode>
    <latex>\#</latex>
    <preferred>\#</preferred>
  </character>
  <character>
    <unicode>$</unicode>
    <latex>\textdollar </latex>
    <mathlatex>\$</mathlatex>
    <preferred>\$</preferred>
  </character>
  <character>
    <unicode>%</unicode>
    <latex>\%</latex>
    <preferred>\%</preferred>
  </character>
  <character>
    <unicode>&amp;</unicode>
    <latex>\&amp;</latex>
    <preferred>\&amp;</preferred>
  </character>
  <character>
    <unicode>~</unicode>
    <latex>\textasciitilde </latex>
    <preferred>\textasciitilde </preferred>
  </character>
  <character>
    <unicode>_</unicode>
    <latex>\_</latex>
    <preferred>\_</preferred>
  </character>
  <character>
    <unicode>^</unicode>
    <latex>\^{}</latex>
    <preferred>\^{}</preferred>
  </character>
  <character>
    <unicode>\</unicode>
    <latex>\textbackslash </latex>
    <mathlatex>\ensuremath{\backslash} </mathlatex>
    <preferred>\ensuremath{\backslash} </preferred>
  </character>
  <character>
    <unicode>{</unicode>
    <latex>\{</latex>
    <preferred>\{</preferred>
  </character>
  <character>
    <unicode>}</unicode>
    <latex>\}</latex>
    <preferred>\}</preferred>
  </character>
  <character>
    <unicode>|</unicode>
    <latex>\ensuremath{\vert} </latex>
    <preferred>\ensuremath{\vert} </preferred>
  </character>
</mapping>
